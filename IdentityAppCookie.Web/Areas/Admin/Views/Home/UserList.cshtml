@model List<AdminUserViewModel>
@{
    ViewData["Title"] = "UserList";
}

<h1>UserList</h1>


<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>User Name</th>
            <th>User Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
            </tr>
        }

    </tbody>
</table>
